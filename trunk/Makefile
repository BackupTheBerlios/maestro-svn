		romain.h \
		mat.h

mat.o: mat.cpp mat.h

romain.o: romain.cpp romain.h

sami.o: sami.cpp sami.h

moc_fabien.o: moc_fabien.cpp  fabien.h 

moc_fenetre.o: moc_fenetre.cpp  fenetre.h imgbox.h \
		fabien.h \
		sami.h \
		romain.h \
		mat.h

moc_imgbox.o: moc_imgbox.cpp  imgbox.h 

moc_mat.o: moc_mat.cpp  mat.h 

moc_romain.o: moc_romain.cpp  romain.h 

moc_sami.o: moc_sami.cpp  sami.h 

moc_fabien.cpp: $(MOC) fabien.h
	$(MOC) fabien.h -o moc_fabien.cpp

moc_fenetre.cpp: $(MOC) fenetre.h
	$(MOC) fenetre.h -o moc_fenetre.cpp

moc_imgbox.cpp: $(MOC) imgbox.h
	$(MOC) imgbox.h -o moc_imgbox.cpp

moc_mat.cpp: $(MOC) mat.h
	$(MOC) mat.h -o moc_mat.cpp

moc_romain.cpp: $(MOC) romain.h
	$(MOC) romain.h -o moc_romain.cpp

moc_sami.cpp: $(MOC) sami.h
